            "class": "App\\Models\\Stock"
        }
    ],
    "validations": [
        {
            "field": "longitud",
            "rules": "required|min:5|max:30"
        },
        {
            "field": "latitud",
            "rules": "required|min:5|max:30"
        }
    ]
	
}