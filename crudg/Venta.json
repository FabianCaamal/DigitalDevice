{
	"model_name":"Venta",
	"soft_deletes":"yes",
	"pk":"id",
	"localize":"yes",
	"table":"venta",
    "fields": [		
        {
            "name": "Fecha",
            "type": "date"
        },
        {
            "name": "Producto",
            "type": "string"
        },
        {
            "name": "Precio",
            "type": "number "
        }
        
    ],
    "foreign_keys": [
        {
            "column": "producto_id",
            "references": "id",
            "on": "productos",
            "onDelete": "cascade"
        },
        {
            "column": "maquina_id",
            "references": "id",
            "on": "maquinas",
            "onDelete": "cascade"
        }
    ],
    "relationships": [
        {
            "name": "producto",
            "type": "hasOne",
            "class": "App\\Models\\Producto"
        },
        {
            "name": "maquina",
            "type": "belongsTo",
            "class": "App\\Models\\Maquina"
        }
    ],

    "validations": [
        {
            "field": "Fecha",
            "rules": "required|min:5|max:20"
        }
    ]
	
}